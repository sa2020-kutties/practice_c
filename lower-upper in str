#include<stdio.h>
#include<string.h>
#define MAX_SIZE 100
int main()
{
    char s[MAX_SIZE];
    int i;
    printf("Enter string 1:");
    gets(s);
    for(i=0;s[i] != '\0';i++)
    {
        if(s[i] >='a' && s[i] <= 'z')
        {
            s[i]=s[i]-32;
        }
    }
    printf("Uppercase is: %s",s);

    
    return 0;
}
