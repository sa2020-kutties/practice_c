#include<stdio.h>
int main()
{
    int a[100],n ,i,largest=a[0];
    printf("Enter size:");
    scanf("%d",&n);
    printf("Enter elements:");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    for(i=1;i<n;i++)
    {
        if(largest > a[i])
            largest=largest;
        else
            largest=a[i];
    }
    printf("Largest num is %d\n",largest);
    return 0;
}
