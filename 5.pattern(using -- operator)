#include <stdio.h>

int main()
{
    int i,j;
    for(i=5;i>=1;i--)
    {
        for(j=i;j>=1;j--)
        {
            printf("* ");
        }
    printf("\n");    
    }

    return 0;
}
* * * * *
* * * *
* * *
* *
*

#include <stdio.h>

int main()
{
    int i,j;
    for(i=5;i>=1;i--)
    {
        for(j=i;j>=1;j--)
        {
            printf("%d ",i);
        }
    printf("\n");    
    }

    return 0;
}
5 5 5 5 5
4 4 4 4
3 3 3
2 2
1

#include <stdio.h>

int main()
{
    int i,j;
    for(i=5;i>=1;i--)
    {
        for(j=i;j>=1;j--)
        {
            printf("%d ",j);
        }
    printf("\n");    
    }

    return 0;
   
}

5 4 3 2 1
4 3 2 1
3 2 1
2 1
1
