#include<stdio.h>
void main()
{
    int n,c,r,sum=0,temp;
    printf("Enter the number:");
    scanf("%d",&n);
    temp=n;
    while(n>0)
    {
        r=n%10;
        c=r*r*r;
        sum=sum+c;
        n=n/10;
    }
    n=temp;
    if(n == sum)
    printf("Armstrong number.");
    else
    printf("Not an Armstrong number.");
}
153
