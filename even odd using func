#include<stdio.h>
int check(int);
void main()
{
    int n,result;
    printf("Enter n:");
    scanf("%d",&n);
    result=check(n);
    if(result == 1)
    {
        printf("Even.");
    }
    else
    {
        printf("Odd.");
    }
    
}
int check(int n)
{
    if(n % 2 == 0)
    {
        return 1;
    }
    else
    {
        return 0;
    }
}
